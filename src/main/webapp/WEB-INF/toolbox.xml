<?xml version="1.0" encoding="UTF-8"?>
<toolbox>
    
    <xhtml>true</xhtml>
    
    <tool>
        <key>alternatorTool</key>
        <scope>application</scope>
        <class>org.apache.velocity.tools.generic.AlternatorTool</class>
    </tool>
    
    <tool>
        <key>contextTool</key>
        <scope>request</scope>
        <class>org.apache.velocity.tools.view.tools.ContextTool</class>
        <parameter name="safe-mode" value="true" />
    </tool>
    
    <tool>
        <key>cookieTool</key>
        <scope>request</scope>
        <class>org.apache.velocity.tools.view.tools.CookieTool</class>
    </tool>
    
    <tool>
        <key>dateTool</key>
        <scope>application</scope>
        <class>org.apache.velocity.tools.generic.DateTool</class>
    </tool>
    
    <tool>
        <key>escapeTool</key>
        <scope>application</scope>
        <class>org.apache.velocity.tools.generic.EscapeTool</class>
    </tool>
    
    <tool>
        <key>importTool</key>
        <scope>request</scope>
        <class>org.apache.velocity.tools.view.tools.ImportTool</class>
    </tool>
    
    <tool>
        <key>iteratorTool</key>
        <scope>request</scope>
        <class>org.apache.velocity.tools.generic.IteratorTool</class>
    </tool>
    
    <tool>
        <key>linkTool</key>
        <scope>request</scope>
        <class>org.apache.velocity.tools.view.tools.LinkTool</class>
    </tool>
    
    <tool>
        <key>listTool</key>
        <scope>application</scope>
        <class>org.apache.velocity.tools.generic.ListTool</class>
    </tool>
    
    <tool>
        <key>mathTool</key>
        <scope>application</scope>
        <class>org.apache.velocity.tools.generic.MathTool</class>
    </tool>
    
    <tool>
        <key>numberTool</key>
        <scope>application</scope>
        <class>org.apache.velocity.tools.generic.NumberTool</class>
    </tool>
    
    <tool>
        <key>parameterParser</key>
        <scope>request</scope>
        <class>org.apache.velocity.tools.view.tools.ParameterParser</class>
    </tool>
    
    <tool>
        <key>renderTool</key>
        <scope>request</scope>
        <class>org.apache.velocity.tools.view.tools.ViewRenderTool</class>
        <parameter name="parse.depth" value="20" />
    </tool>
    
    <tool>
        <key>resourceTool</key>
        <scope>request</scope>
        <class>org.apache.velocity.tools.view.tools.ViewResourceTool</class>
    </tool>
    
</toolbox>
