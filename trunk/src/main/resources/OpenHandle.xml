<?xml version="1.0" encoding="UTF-8"?>
<openhandle>
    <servlet>
        <parameterNames>
            <handleId>id</handleId>
            <datatypes>include</datatypes>
            <format>format</format>
            <index>index</index>
            <type>type</type>
            <compact>compact</compact>
            <mimetype>mimetype</mimetype>
            <callback>callback</callback>
        </parameterNames>
        <templates>
            <handleResponse>
                <template>
                    <name>/handle</name>
                    <mimetype>application/json</mimetype>
                </template>
                <template>
                    <name>/handle</name>
                    <mimetype>text/rdf+n3</mimetype>
                </template>
                <template>
                    <name>/handle</name>
                    <mimetype>application/rdf+xml</mimetype>
                </template>
            </handleResponse>
            <errorResponse>
                <template>
                    <name>/error</name>
                    <mimetype>application/json</mimetype>
                </template>
                <template>
                    <name>/error</name>
                    <mimetype>text/rdf+n3</mimetype>
                </template>
                <template>
                    <name>/error</name>
                    <mimetype>application/rdf+xml</mimetype>
                </template>
            </errorResponse>
        </templates>
    </servlet>
    <traceMessages>true</traceMessages>
    <preferredProtocols>http, tcp</preferredProtocols>
</openhandle>
