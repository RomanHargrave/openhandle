#set ($id = $request.getParameter("id"))
#set ($this = $request.getAttribute("id"))
#set ($err = $request.getAttribute("error"))
# OpenHandle (RDF/N3) - see http://code.google.com/p/openhandle/

@prefix h: <http://hdl.handle.net/10100/handle.rdfs#> .
@prefix r: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
 
<hdl:${id}> a h:Handle ;
    h:handleStatus [
        h:code "${err.code}" ;
        h:message "${err.message}" ;
    ] ;
    h:handleValues ( ) .
